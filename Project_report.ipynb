   "cell_type": "code",
   "execution_count": 219,
   "metadata": {},
   "outputs": [],
   "source": [
    "final_dataset['TFIDF'] = tfidf_vectorization\n",
    "final_dataset['OneHot'] = onehot_vectorization"
   ]
